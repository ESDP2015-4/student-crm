
<% @result.each do |ch| %>
    <% ch.parents.each do |parent| %>
        <% if file.id == parent.id %>
            <% if ch.mimeType == 'application/vnd.google-apps.folder' %>
                <div class="panel panel-default">
                  <div class="panel-heading" role="tab" id="headingTwo">
                    <h4 class="panel-title">
                      <div class="row">
                        <div class="col-md-8">
                          <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#<%= ch.id %>" aria-expanded="false" aria-controls="collapseTwo">
                            <%= image_tag ch.icon_link %>  <%= ch.title %>
                          </a>
                        </div>
                        <div class="col-md-2">
                          <%= link_to 'Открыть', ch.alternate_link, class: 'btn btn-default btn-sm', :target => "_blank" %>
                        </div>
                        <div class="col-md-2">
                          <%= render 'form_hidden', file: ch %>
                        </div>
                      </div>
                    </h4>
                  </div>
                  <div id="<%= ch.id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                    <div class="panel-body">
                      <%= render 'new_readings', file: ch %>
                    </div>
                  </div>
                </div>
            <% else %>
                <div class="panel">
                  <div class="panel-heading" role="tab" id="headingTwo">
                    <h4 class="panel-title">
                      <div class="row">
                        <div class="col-md-8">
                          <div class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" aria-expanded="false" aria-controls="collapseTwo">
                            <%= image_tag ch.icon_link %>  <%= ch.title %>
                          </div>
                        </div>
                        <div class="col-md-2">
                          <%= link_to 'Открыть', ch.alternate_link, class: 'btn btn-default btn-sm', :target => "_blank" %>
                        </div>
                        <div class="col-md-2">
                          <%= render 'form_hidden', file: ch %>
                        </div>
                      </div>
                    </h4>
                  </div>
                </div>
            <% end %>
        <% end %>
    <% end %>
<% end %>

