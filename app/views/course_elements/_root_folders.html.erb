<div class="panel">
  <div class="panel-heading" role="tab" id="headingTwo">
    <h4 class="panel-title">
      <div class="row">
        <div class="col-md-8">
          <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#<%= f.id %>" aria-expanded="false" aria-controls="collapseTwo">
            <%= image_tag f.icon_link %>  <%= f.title %>
          </a>
        </div>
        <!-- this is your folder in the root of your disk - 'My Disk' -->
        <div class="col-md-2">
          <%= link_to 'Открыть', f.alternate_link, class: 'btn btn-default btn-sm', :target => "_blank" %>
        </div>
        <div class="col-md-2">
          <%= render 'form_hidden', file: f %>
        </div>
      </div>
    </h4>
  </div>
  <div id="<%= f.id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
    <div class="panel-body">
      <%= render 'new_readings', file: f %>
    </div>
  </div>
</div>